{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "author": "Andrew Hart",
  "version": "4.5",
  "homepage_url": "https://github.com/hartag/keynav",
  "default_locale": "en",

  "browser_specific_settings": {
    "gecko": {
      "id": "keynav@andrew.hart",
      "strict_min_version": "115.0"
    }
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "commands": {
    "folder-quick-nav": {
      "suggested_key": {
        "default": "F4"
      },
      "description": "Open Quick Folder Key Navigation Popup"
    }
  },

  "permissions": [
    "accountsRead", 
    "storage"
  ],

  "browser_action": {
    "default_title": "__MSG_actionButtonLabel__",
    "default_popup": "/popup/popup.html"
  }
}
